console.log( string );

}

function get_five_character_length_string(array) {
  var srtring = array[0];
  for (i = 0; i < array.length; i++) {
    if (array[i].length == 5 ) {
      srtring = array[i];
      break;
    }
  }
  return srtring;
}


</script>

